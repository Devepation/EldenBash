echo "Installing Elden Bash Beta 2"
cd ~

# old thing touch /bin/bash/play-eldenbash
wget https://github.com/Devepation/EldenBash/raw/main/src/eldenring-beta-2
chmod +x eldenring-beta-2
sudo mv eldenring-beta-2 /bin/eldenbash
cd ~
echo "Game successfully installed/upgraded! launch with the command: eldenbash"