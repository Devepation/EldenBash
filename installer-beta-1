echo "Installing Elden Bash Beta 1"
cd ~

# old thing touch /bin/bash/play-eldenbash
wget https://github.com/Devepation/EldenBash/raw/main/src/eldenring-beta-1
chmod +x eldenring-beta-1
sudo mv eldenring-beta-1 /bin/eldenbash
cd ~
echo "Game successfully installed/upgraded! launch with the command: eldenbash"